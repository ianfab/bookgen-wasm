{
  "scripts": {
    "link-lib": "rm -rf public/lib && ln -s $PWD/node_modules/bookgen.wasm public/lib",
    "copy-lib": "rm -rf public/lib && mkdir -p public/lib && cp node_modules/bookgen.wasm/* public/lib",
    "serve": "serve public --no-compression",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "vercel deploy public --prod"
  },
  "dependencies": {
    "prettier": "^2.3.2",
    "serve": "^14.2.5",
    "bookgen.wasm": "1.1.10",
    "vercel": "^23.0.1"
  }
}
